
# Replace these values with your email provider's IMAP settings

# Your email provider's IMAP server address (e.g., imap.gmail.com for Gmail)
IMAP_SERVER=your.imap.server.com

IMAP_PORT=993

# Your email address
IMAP_USER=your.email@example.com

# For Gmail, you need to generate an App Password in your Google Account settings

IMAP_PASSWORD=your_app_specific_password

IMAP_TLS=true
